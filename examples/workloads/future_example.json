{
  "api_version": "v0",
  "kind": "pods",
  "name": "node",
  "spec": {
    "containers": [
      {
        "name": "node",
        "image": "node:slim",
        "env": [
          {
            "name": "key1",
            "value": "value1"
          },
          {
            "name": "key2",
            "value": "value2"
          }
        ],
        "ports": {
          "port": 80,
          "target_port": 80,
          "protocol": "TCP",
          "type": "clusterIP|nodePort|loadBalancer"
        }
      }
    ]
  }
}